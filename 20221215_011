#include <stdio.h>

int main() 
{
    float sh, h, ir, sb, inss, sind, sl;
    
    printf("Quanto voce ganha por hora: ");
    scanf("%f", &sh);
    printf("Quantas horas voce trabalha ao mês: ");
    scanf("%f", &h);
    
    sb = sh * h;
    ir = sb * 11 / 100;
    inss = sb * 8 / 100;
    sind = sb * 5 / 100;
    sl = sb - ir - inss - sind;
    
    printf("Salário Bruto: R$%.2f \n", sb);
    printf("Imposto de Renda: R$%.2f \n", ir);
    printf("INSS: R$%.2f \n", inss);
    printf("Sindicato: R$%.2f \n", sind);
    printf("Salário Líquido: R$%.2f", sl);
}
