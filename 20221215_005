{
    float lado, area, area2;
    printf("AREA DE UM QUADRADO\n");
    printf("Digite a medida do lado: ");
    scanf("%f", &lado); 
    
    area = lado * lado;
    area2 = area * 2;

    printf("O dobro da Ã¡rea do quadrado vale: %f", area2);
    return 0;
}
